{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "moduleResolution": "node",
    "rootDir": "./src",
    "outDir": "../../dist/apps/provider",
    "composite": true,
    "declaration": true,
    "incremental": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "useDefineForClassFields": false,
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "../../"
  },
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "../../libs/types"
    },
    {
      "path": "../../libs/utils"
    },
    {
      "path": "../../libs/key"
    },
    {
      "path": "../../libs/config"
    },
    {
      "path": "../../libs/plugin-driver"
    },
    {
      "path": "../../libs/connectors"
    },
    {
      "path": "../../libs/orders"
    },
    {
      "path": "../../libs/provider-ws-bridge"
    },
    {
      "path": "../../libs/telegram"
    },
    {
      "path": "../../libs/detector"
    }
  ]
}